                margin: 0px;
                border-top: 1px dashed black;
            }

            .text-caps {
                text-transform: uppercase;
            }

            .text-red
            {
                color: red !important;
            }

            .margin-top-small {
                margin-top: 12px;
            }

            .text-highlight{
                background-color: yellow !important;
                font-size: 13px !important;
            }
